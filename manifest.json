{
	"manifest_version": 2,
	"name": "Spigot Sales Graph",
	"description": "See graph/total gained and total sales in your premium spigot resources",
	"version": "1.0.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"permissions": [
		"tabs", "https://www.spigotmc.org/"
	],

	"content_scripts": [{
		"matches": ["https://www.spigotmc.org/*/buyers"],
		"js": ["jquery-3.1.1.min.js", "Chart.js", "chartjs-plugin-zoom.js", "myscript.js"]
	}]
}